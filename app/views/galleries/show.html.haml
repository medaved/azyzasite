.container
 .gallery-feed
  - @images.each do |image|
   .col-xs-12.col-sm-4.col-md-3.col-lg-3
    - if image.fileimage.present?
     = link_to image.fileimage.url, class: "img-thumbnail", data: { title: image.caption, lightbox: @gallery.name } do
      = image_tag image.fileimage.thumb.url , class: 'img-responsive'